<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="CampaignScriptEditor.Editors.Fields.SerializableLocationField">
	<StackPanel>
		<TextBlock Name="Header"/>
		<StackPanel Name="SubFields" Margin="8,0">
			<CheckBox Name="UseSceneLookUp" Content="UseSceneLookUp"/>
			<TextBlock Text="LookUpName"/>
			<TextBox Name="LookUpName"/>
		</StackPanel>
	</StackPanel>
</UserControl>
